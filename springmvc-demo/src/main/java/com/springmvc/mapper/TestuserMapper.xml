<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.springmvc.mapper.TestuserMapper">        
    <insert id="save" parameterType="com.springmvc.domain.Testuser" useGeneratedKeys="true">
        insert into
        tb_user (username,sex,age)
        values(#{username},#{sex},#{age})       
    </insert>
</mapper>